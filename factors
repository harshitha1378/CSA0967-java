import java.util.*;
import java.util.Scanner;

class factors
{
public static void main(String[] args)
{
Scanner input=new Scanner(System.in);
System.out.print("enter a number:");
int num=input.nextInt();
System.out.print("enter Nth number:");
int n=input.nextInt();
int count=0;
for(int i=1; i<=num;i++)
{
if(num%i==0)
{
count=count+1;
if(count==n)
{
System.out.println(n+"th factor of "+num+" is "+i);
}
}
}
System.out.println("total no.of factors of "+num+" is "+count);
}
}
