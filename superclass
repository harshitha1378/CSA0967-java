import java.util.Scanner;

class subclass

{
   	
int num;
   	
int flt;
   	
subclass(int num,int  flt)
	
{
      		
this.num = num;
      		
this.flt = flt;
   	
}
	
public static void main(String args[])
	
{
      		

Scanner sc = new Scanner(System.in);
      		
System.out.print("Enter integer : ");
		
if(!sc.hasNextInt())
		
{
			
System.out.print("Invalid");
			
return;
		
}
      		
int num = sc.nextInt();
      		
System.out.print("Enter integer: ");
		
if(!sc.hasNextInt())
		
{
			
System.out.print("Invalid");
			
return;
		
}
      		
int flt = sc.nextInt();      
      		
subclass obj = new subclass(num, flt);
      		
System.out.print(obj.num);
      		
System.out.println(obj.flt);
   
}

}
