import java.util.Scanner;
class righttri {
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        System.out.print("enter no.of lines");
        int n=sc.nextInt();
        int a, b;
        for(a = 0; a < n; a++) 
	{
		for(b = 0; b <= a; b++)
		{
                System.out.print("* ");
            	}
        System.out.println();
	}
}
}
